    "Alfredo Miranda <alfredo@miranda.io>"
  ],
  "bugs": {
    "url": "https://github.com/opentargets/genetics/issues",
    "email": "support@targetvalidation.org"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.2",
    "apollo-boost": "^0.1.10",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-schema": "^1.1.0",
    "casual-browserify": "^1.5.19-2",
    "classnames": "^2.2.6",
    "d3": "^5.5.0",
    "fg-loadcss": "^2.0.1",
    "file-saver": "^1.3.8",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.5",
    "lodash": "^4.17.10",
    "ot-charts": "^0.0.17",
    "ot-ui": "^0.0.20",
    "polished": "^1.9.3",
    "query-string": "5",
    "react": "^16.4.2",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-measure": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-select": "^2.0.0",
    "react-virtualized": "^9.20.1",
    "source-map-explorer": "^1.5.0",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "REACT_APP_REVISION=\"$(git rev-parse --short HEAD)\" react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "analyse": "source-map-explorer build/static/js/main.*",
    "prettier": "prettier --write",
    "prettier:all": "prettier --write 'src/**/*.js' config-overrides.js",
    "eslint": "eslint . --fix --ext .js",
    "version": "yarn build",
    "report:coverage": "codecov"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.8.0",
    "codecov": "^3.0.4",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "1.14.0",
    "react-app-rewire-inline-import-graphql-ast": "^1.1.1",
    "react-app-rewired": "^1.5.2",
    "react-test-renderer": "^16.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "**/*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js"
    ]
  }
}
